{
    "manifest_version": 3,
    "name": "ChatGPT to Markdown",
    "version": "1.0",
    "description": "Export ChatGPT conversation to Markdown file with toggleable questions.",
    "permissions": [
        "activeTab",
        "storage",
        "scripting"
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons": {
    "16": "icons/export_16.png",
    "32": "icons/export_32.png",
    "64": "icons/export_64.png",
    "128": "icons/export_128.png"
    }
    
}